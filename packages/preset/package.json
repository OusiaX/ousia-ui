{
  "name": "@ousia-ui/preset",
  "version": "0.0.1",
  "description": "Panda CSS Preset for Ousia UI",
  "keywords": ["Ark UI", "Panda CSS", "Preset", "Park UI", "Ousia UI"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OusiaX/ousia-ui.git",
    "directory": "packages/preset"
  },
  "bugs": {
    "url": "https://github.com/OusiaX/ousia-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "release-it": "release-it",
    "lint": "biome lint ./src",
    "typecheck": "tsc",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "effect": "3.10.19"
  },
  "devDependencies": {
    "@pandacss/dev": "0.53.4",
    "@pandacss/types": "1.3.0",
    "@release-it/keep-a-changelog": "5.0.0",
    "clean-package": "2.2.0",
    "release-it": "17.10.0",
    "tsup": "8.3.5",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/colors/*.ts",
      "src/utils/index.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}